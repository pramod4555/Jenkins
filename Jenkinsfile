pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World how are you'
            }
        }
   stage('Hello hi') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello how are you') {
            steps {
                echo 'Hello World the outer message'
            }
        }

    }
    }